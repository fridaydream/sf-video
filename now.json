{
  "version": 2,
  "builds": [
    {
      "src": "server/server.ts",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/list",
      "dest": "server/server.ts"
    },
    {
      "src": "/playing",
      "dest": "server/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.ts"
    }
  ]
}
